
module.exports = {
  /** wxs格式化：驼峰转中划线 */
  humpToLine: function (str) {
    if (typeof str !== 'string') str += ''
    return str.split('').map(function (item, idx) {
      if (item.toUpperCase() === item) {
        return idx === 0 ? item.toLowerCase() : '-' + item.toLowerCase()
      } else {
        return item
      }
    }).join('')
  }
}
